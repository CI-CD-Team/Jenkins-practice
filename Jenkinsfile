pipeline {
   agent any

   stages {
      stage("build") {
        steps {
          echo 'building our app to test...'
        }
      }

      stage("test") {
        steps {
         echo 'testing our app...'
        }
      }

      stage("deploy") {
        steps {
         echo 'deploying our app...'
        }
      }

   }
}